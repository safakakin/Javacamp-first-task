package loopDemo;

import java.util.Iterator;

public class Main {

	public static void main(String[] args) {
		for (int i = 1; i < 10; i++) {
			System.out.println(i);
		}
		System.out.println("Döngü bitti"); 
		
		int i=1;
		while(i<10) {
			i++;
		}
		System.out.println("While döngüsü bitti"); 

	}
	

}
