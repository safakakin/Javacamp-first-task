package sayiBulma;

public class Main {

	public static void main(String[] args) {
		int[] sayilar = { 1, 2, 5, 7, 9, 0 };
		int aranacak = 5;

		for (int sayi : sayilar) {
			if (sayi == aranacak) {
				System.out.println("Sayı listede mevcuttur.");
				return;
			}
		}
		System.out.println("Sayı listede mevcut değildir.");

	}

}
